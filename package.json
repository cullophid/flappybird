{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:js": "browserify src/js/main.js -t babelify -d -o public/bundle.js --extension .jsx",
    "build:sass": "node-sass src/sass/bundle.scss -o public/",
    "build": "npm run build:js && npm run build:sass",
    "watch": "parallelshell 'watch \"npm run build:js\" src/js' 'watch \" npm run build:sass\" src/sass'",
    "livereload": "parallelshell 'livereload public' 'npm run watch'",
    "start": "node main.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "body-parser": "~1.10.2",
    "config": "^1.17.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.1.1",
    "express": "~4.11.1",
    "morgan": "^1.6.1",
    "ramda": "^0.18.0",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "eslint": "^1.10.0",
    "eslint-plugin-react": "^3.9.0",
    "history": "^1.13.1",
    "livereload": "^0.4.0",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "watch": "^0.16.0"
  }
}
